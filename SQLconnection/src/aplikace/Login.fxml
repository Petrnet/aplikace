<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aplikace.LoginController">
   <children>
      <Label fx:id="isConnected" layoutX="83.0" layoutY="39.0" prefHeight="90.0" prefWidth="127.0" text="Status" textFill="#eb0707">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtUsername" layoutX="61.0" layoutY="138.0" promptText="User name" />
      <PasswordField fx:id="txtPassword" layoutX="61.0" layoutY="185.0" promptText="Password" />
      <Button layoutX="110.0" layoutY="237.0" mnemonicParsing="false" onAction="#Login" text="Login" />
   </children>
</AnchorPane>
